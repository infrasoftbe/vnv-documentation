{
  "entryFile": "src/api/index.ts",
  "controllerPathGlobs": [
    "src/api/controllers/database/**/*.ts"
  ],
  "spec": {
    "outputDirectory": "./openapi/database",
    "specVersion": 3,
    "outputFileName": "database.spec.json",
    "securityDefinitions": {
      "api_key": {
        "type": "apiKey",
        "name": "access_token",
        "in": "query"
      }
    }
  }
}